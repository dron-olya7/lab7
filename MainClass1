public class MainClass1 {


	public static void main(String[] args) {
		
		 SuperClassTest1 superClassObject = new SuperClassTest1("передал в конструктор суперкласса");
		 String className = superClassObject.toString();
		 System.out.println(className);
		 
		 SubClassTest1 subClassObject1 = new SubClassTest1("передал в конструктор подкласса");
		 String subClassName = subClassObject1.toString();
		 System.out.println(subClassName);
		 
		 SubClassTest1 subClassObject2 = new SubClassTest1("передал в конструктор подкласса", "где два параметра");
		 String subClassName2 = subClassObject2.toString();
		 System.out.println(subClassName2);
		 }
		}
